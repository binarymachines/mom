<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://browseCaption"
        class="com.angrysurfer.mildred.web.suggestion.generatedactionparam.GeneratedActionParamBrowse"
        focusComponent="generatedActionParamsTable"
        lookupComponent="generatedActionParamsTable"
        messagesPack="com.angrysurfer.mildred.web.suggestion.generatedactionparam">
    <dsContext>
        <groupDatasource id="generatedActionParamsDs"
                         class="com.angrysurfer.mildred.entity.suggestion.GeneratedActionParam"
                         view="GeneratedActionParam-screen-view">
            <query>
                <![CDATA[select e from mildred$GeneratedActionParam e]]>
            </query>
        </groupDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="generatedActionParamsTable"
            spacing="true">
        <filter id="filter"
                applyTo="generatedActionParamsTable"
                datasource="generatedActionParamsDs">
            <properties include=".*"/>
        </filter>
        <groupTable id="generatedActionParamsTable"
                    width="100%">
            <actions>
                <action id="create"/>
                <action id="edit"/>
                <action id="remove"/>
            </actions>
            <columns>
                <column id="action"/>
                <column id="value"/>
            </columns>
            <rows datasource="generatedActionParamsDs"/>
            <rowsCount/>
            <buttonsPanel id="buttonsPanel"
                          alwaysVisible="true">
                <button id="createBtn"
                        action="generatedActionParamsTable.create"/>
                <button id="editBtn"
                        action="generatedActionParamsTable.edit"/>
                <button id="removeBtn"
                        action="generatedActionParamsTable.remove"/>
            </buttonsPanel>
        </groupTable>
    </layout>
</window>
