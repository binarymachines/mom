<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.angrysurfer.mildred.web.service.modestatedefault.ModeStateDefaultEdit"
        datasource="modeStateDefaultDs"
        focusComponent="fieldGroup"
        messagesPack="com.angrysurfer.mildred.web.service.modestatedefault">
    <dsContext>
        <datasource id="modeStateDefaultDs"
                    class="com.angrysurfer.mildred.entity.ModeStateDefault"
                    view="ModeStateDefault-screen-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="modeStateDefaultDs">
            <column width="250px">
                <field property="mode"/>
                <field property="state"/>
                <field property="priority"/>
                <field property="effectDispatch"/>
                <field property="timesToComplete"/>
                <field property="decPriorityAmount"/>
                <field property="incPriorityAmount"/>
                <field property="errorTolerance"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="extendedEditWindowActions"/>
    </layout>
</window>
