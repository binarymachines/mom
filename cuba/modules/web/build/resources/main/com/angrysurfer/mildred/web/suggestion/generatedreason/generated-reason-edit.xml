<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.angrysurfer.mildred.web.suggestion.generatedreason.GeneratedReasonEdit"
        datasource="generatedReasonDs"
        focusComponent="fieldGroup"
        messagesPack="com.angrysurfer.mildred.web.suggestion.generatedreason">
    <dsContext>
        <datasource id="generatedReasonDs"
                    class="com.angrysurfer.mildred.entity.suggestion.GeneratedReason"
                    view="GeneratedReason-screen-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="generatedReasonDs">
            <column width="250px">
                <field property="parent"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
