<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.angrysurfer.mildred.web.media.Category.CategoryEdit"
        datasource="CategoryDs"
        focusComponent="fieldGroup"
        messagesPack="com.angrysurfer.mildred.web.media.Category">
    <dsContext>
        <datasource id="CategoryDs"
                    class="com.angrysurfer.mildred.entity.media.Category"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="CategoryDs">
            <column width="250px">
                <field property="name"/>
                <field property="assetType"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
