<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.angrysurfer.mildred.web.suggestion.generatedreasonparam.GeneratedReasonParamEdit"
        datasource="generatedReasonParamDs"
        focusComponent="fieldGroup"
        messagesPack="com.angrysurfer.mildred.web.suggestion.generatedreasonparam">
    <dsContext>
        <datasource id="generatedReasonParamDs"
                    class="com.angrysurfer.mildred.entity.suggestion.GeneratedReasonParam"
                    view="GeneratedReasonParam-screen-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="generatedReasonParamDs">
            <column width="250px">
                <field property="reason"/>
                <field property="value"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
