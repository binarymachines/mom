<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.angrysurfer.mildred.web.suggestion.generatedactionparam.GeneratedActionParamEdit"
        datasource="generatedActionParamDs"
        focusComponent="fieldGroup"
        messagesPack="com.angrysurfer.mildred.web.suggestion.generatedactionparam">
    <dsContext>
        <datasource id="generatedActionParamDs"
                    class="com.angrysurfer.mildred.entity.suggestion.GeneratedActionParam"
                    view="GeneratedActionParam-screen-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="generatedActionParamDs">
            <column width="250px">
                <field property="action"/>
                <field property="value"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
