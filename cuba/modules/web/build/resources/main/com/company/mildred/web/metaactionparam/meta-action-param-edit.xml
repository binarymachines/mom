<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.mildred.web.metaactionparam.MetaActionParamEdit"
        datasource="metaActionParamDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.mildred.web.metaactionparam">
    <dsContext>
        <datasource id="metaActionParamDs"
                    class="com.company.mildred.entity.MetaActionParam"
                    view="MetaActionParam-screen-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="metaActionParamDs">
            <column width="250px">
                <field property="metaAction"/>
                <field property="vectorParam"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
