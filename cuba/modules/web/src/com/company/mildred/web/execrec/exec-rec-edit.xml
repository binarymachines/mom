<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.mildred.web.execrec.ExecRecEdit"
        datasource="execRecDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.mildred.web.execrec">
    <dsContext>
        <datasource id="execRecDs"
                    class="com.company.mildred.entity.ExecRec"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="execRecDs">
            <column width="250px">
                <field property="pid"/>
                <field property="status"/>
                <field property="startDt"/>
                <field property="endDt"/>
                <field property="effectiveDt"/>
                <field property="expirationDt"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
