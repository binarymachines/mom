<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.mildred.web.metareasonparam.MetaReasonParamEdit"
        datasource="metaReasonParamDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.mildred.web.metareasonparam">
    <dsContext>
        <datasource id="metaReasonParamDs"
                    class="com.company.mildred.entity.MetaReasonParam"
                    view="MetaReasonParam-screen-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="metaReasonParamDs">
            <column width="250px">
                <field property="metaReason"/>
                <field property="vectorParam"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
