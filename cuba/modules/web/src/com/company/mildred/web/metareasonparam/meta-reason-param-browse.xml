<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://browseCaption"
        class="com.company.mildred.web.metareasonparam.MetaReasonParamBrowse"
        focusComponent="metaReasonParamsTable"
        lookupComponent="metaReasonParamsTable"
        messagesPack="com.company.mildred.web.metareasonparam">
    <dsContext>
        <groupDatasource id="metaReasonParamsDs"
                         class="com.company.mildred.entity.MetaReasonParam"
                         view="MetaReasonParam-screen-view">
            <query>
                <![CDATA[select e from mildred$MetaReasonParam e]]>
            </query>
        </groupDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="metaReasonParamsTable"
            spacing="true">
        <filter id="filter"
                applyTo="metaReasonParamsTable"
                datasource="metaReasonParamsDs">
            <properties include=".*"/>
        </filter>
        <groupTable id="metaReasonParamsTable"
                    width="100%">
            <actions>
                <action id="create"/>
                <action id="edit"/>
                <action id="remove"/>
            </actions>
            <columns>
                <column id="metaReason"/>
                <column id="vectorParam"/>
            </columns>
            <rows datasource="metaReasonParamsDs"/>
            <rowsCount/>
            <buttonsPanel id="buttonsPanel"
                          alwaysVisible="true">
                <button id="createBtn"
                        action="metaReasonParamsTable.create"/>
                <button id="editBtn"
                        action="metaReasonParamsTable.edit"/>
                <button id="removeBtn"
                        action="metaReasonParamsTable.remove"/>
            </buttonsPanel>
        </groupTable>
    </layout>
</window>
