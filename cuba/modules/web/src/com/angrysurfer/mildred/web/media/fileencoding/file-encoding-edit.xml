<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.angrysurfer.mildred.web.media.fileencoding.FileEncodingEdit"
        datasource="fileEncodingDs"
        focusComponent="fieldGroup"
        messagesPack="com.angrysurfer.mildred.web.media.fileencoding">
    <dsContext>
        <datasource id="fileEncodingDs"
                    class="com.angrysurfer.mildred.entity.media.FileEncoding"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="fileEncodingDs">
            <column width="250px">
                <field property="name"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
