<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editCaption"
        class="com.angrysurfer.mildred.cuba.primary.web.filehandlertype.FileHandlerTypeEdit"
        datasource="fileHandlerTypeDs"
        focusComponent="fieldGroup"
        messagesPack="com.angrysurfer.mildred.cuba.primary.web.filehandlertype">
    <dsContext>
        <datasource id="fileHandlerTypeDs"
                    class="com.angrysurfer.mildred.cuba.primary.entity.FileHandlerType"
                    view="FileHandlerType-screen-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="fileHandlerTypeDs">
            <column width="250px">
                <field id="fileHandler"/>
                <field id="ext"/>
                <field id="name"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
