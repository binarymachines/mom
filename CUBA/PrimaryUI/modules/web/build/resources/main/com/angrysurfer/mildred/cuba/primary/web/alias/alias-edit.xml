<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editCaption"
        class="com.angrysurfer.mildred.cuba.primary.web.alias.AliasEdit"
        datasource="aliasDs"
        focusComponent="fieldGroup"
        messagesPack="com.angrysurfer.mildred.cuba.primary.web.alias">
    <dsContext>
        <datasource id="aliasDs"
                    class="com.angrysurfer.mildred.cuba.primary.entity.Alias"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="aliasDs">
            <column width="250px">
                <field id="name"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
