<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editCaption"
        class="com.angrysurfer.mildred.cuba.actions.web.actiondispatch.ActionDispatchEdit"
        datasource="actionDispatchDs"
        focusComponent="fieldGroup"
        messagesPack="com.angrysurfer.mildred.cuba.actions.web.actiondispatch">
    <dsContext>
        <datasource id="actionDispatchDs"
                    class="com.angrysurfer.mildred.cuba.actions.entity.ActionDispatch"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="actionDispatchDs">
            <column width="250px">
                <field id="name"/>
                <field id="category"/>
                <field id="packageName"/>
                <field id="moduleName"/>
                <field id="className"/>
                <field id="funcName"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
