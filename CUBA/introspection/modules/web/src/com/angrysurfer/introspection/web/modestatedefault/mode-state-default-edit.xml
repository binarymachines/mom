<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.angrysurfer.introspection.web.modestatedefault.ModeStateDefaultEdit"
        datasource="modeStateDefaultDs"
        focusComponent="fieldGroup"
        messagesPack="com.angrysurfer.introspection.web.modestatedefault">
    <dsContext>
        <datasource id="modeStateDefaultDs"
                    class="com.angrysurfer.introspection.entity.ModeStateDefault"
                    view="ModeStateDefault-screen-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="modeStateDefaultDs">
            <column width="250px">
                <field id="mode"/>
                <field id="state"/>
                <field id="effectDispatch"/>
                <field id="errorTolerance"/>
                <field id="decPriorityAmount"/>
                <field id="incPriorityAmount"/>
                <field id="timeToComplete"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
