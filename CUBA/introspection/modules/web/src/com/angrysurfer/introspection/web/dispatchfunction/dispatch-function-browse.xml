<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://browseCaption"
        class="com.angrysurfer.introspection.web.dispatchfunction.DispatchFunctionBrowse"
        focusComponent="dispatchFunctionsTable"
        lookupComponent="dispatchFunctionsTable"
        messagesPack="com.angrysurfer.introspection.web.dispatchfunction">
    <dsContext>
        <groupDatasource id="dispatchFunctionsDs"
                         class="com.angrysurfer.introspection.entity.DispatchFunction"
                         view="_local">
            <query>
                <![CDATA[select e from introspection$DispatchFunction e]]>
            </query>
        </groupDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="dispatchFunctionsTable"
            spacing="true">
        <filter id="filter"
                applyTo="dispatchFunctionsTable"
                datasource="dispatchFunctionsDs">
            <properties include=".*"/>
        </filter>
        <groupTable id="dispatchFunctionsTable"
                    width="100%">
            <actions>
                <action id="create"/>
                <action id="edit"/>
                <action id="remove"/>
            </actions>
            <columns>
                <column id="name"/>
                <column id="category"/>
                <column id="packageName"/>
                <column id="moduleName"/>
                <column id="className"/>
                <column id="funcName"/>
            </columns>
            <rows datasource="dispatchFunctionsDs"/>
            <rowsCount/>
            <buttonsPanel id="buttonsPanel"
                          alwaysVisible="true">
                <button id="createBtn"
                        action="dispatchFunctionsTable.create"/>
                <button id="editBtn"
                        action="dispatchFunctionsTable.edit"/>
                <button id="removeBtn"
                        action="dispatchFunctionsTable.remove"/>
            </buttonsPanel>
        </groupTable>
    </layout>
</window>
