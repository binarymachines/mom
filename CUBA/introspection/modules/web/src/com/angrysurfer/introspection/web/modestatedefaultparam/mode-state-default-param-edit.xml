<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.angrysurfer.introspection.web.modestatedefaultparam.ModeStateDefaultParamEdit"
        datasource="modeStateDefaultParamDs"
        focusComponent="fieldGroup"
        messagesPack="com.angrysurfer.introspection.web.modestatedefaultparam">
    <dsContext>
        <datasource id="modeStateDefaultParamDs"
                    class="com.angrysurfer.introspection.entity.ModeStateDefaultParam"
                    view="ModeStateDefaultParam-screen-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="modeStateDefaultParamDs">
            <column width="250px">
                <field id="name"/>
                <field id="value"
                       rows="5"/>
                <field id="modeStateDefault"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
