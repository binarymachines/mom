<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.angrysurfer.introspection.web.dispatchfunction.DispatchFunctionEdit"
        datasource="dispatchFunctionDs"
        focusComponent="fieldGroup"
        messagesPack="com.angrysurfer.introspection.web.dispatchfunction">
    <dsContext>
        <datasource id="dispatchFunctionDs"
                    class="com.angrysurfer.introspection.entity.DispatchFunction"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="dispatchFunctionDs">
            <column width="250px">
                <field id="name"/>
                <field id="category"/>
                <field id="packageName"/>
                <field id="moduleName"/>
                <field id="className"/>
                <field id="funcName"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
